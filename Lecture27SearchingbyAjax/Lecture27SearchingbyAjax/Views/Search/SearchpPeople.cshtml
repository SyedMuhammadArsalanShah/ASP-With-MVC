@model IEnumerable<Lecture27SearchingbyAjax.Models.info>

@{
    ViewBag.Title = "SearchpPeople";
}

<h2>SearchpPeople</h2>


@foreach (var i in Model)
{
    <tr>
        <td>@i.Id</td>
        <td>@i.Name</td>
        <td>@i.Age</td>
        <td>@i.Salary</td>
        <td>@i.Contact</td>
    </tr>



}